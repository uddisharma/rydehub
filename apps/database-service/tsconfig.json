{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDirs": ["./src", "./prisma", "./tests"],
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
